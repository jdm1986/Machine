@model Machine.Models.MachineViewModel
@{
    ViewBag.Title = "Machine Detail";
}
 
<h2>@Model.MachineMakeModel</h2>


@*Machine detail view*@
<table class="table table-striped">
    <thead>
        <tr>
            <th>@Html.LabelFor(m => m.MachineMake)</th>
            <th>@Html.LabelFor(m => m.MachineModel)</th>
            <th>@Html.LabelFor(m => m.MachineNum)</th>
            <th>@Html.LabelFor(m => m.TypeId)</th>
            <th>@Html.LabelFor(m => m.Hours)</th>
   
        </tr>
    </thead>
        <tbody>
            <tr>
                <td>@Html.DisplayFor(m => m.MachineMake)</td>
                <td>@Html.DisplayFor(m => m.MachineModel)</td>
                <td>@Html.DisplayFor(m => m.MachineNum)</td>
                @*need to change this from id to TypeName*@
                <td>@Html.DisplayFor(m => m.TypeId)</td>
                <td>@Html.DisplayFor(m => m.Hours)</td>
   
            </tr>
        </tbody>
    
</table>
<div class="col-sm-12">
    @Html.ActionLink("Return to Machine List", "Index", new { @class = "btn btn-primary"})
</div>